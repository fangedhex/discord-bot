    "jest-mock-extended": "^1.0.10",
    "prettier": "^2.1.1",
    "rollup": "^2.6.1",
    "rollup-plugin-progress": "^1.1.1",
    "ts-jest": "^25.4.0",
    "ts-node-dev": "^1.0.0-pre.44",
